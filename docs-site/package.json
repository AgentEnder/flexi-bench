{
  "name": "@flexi-bench/docs-site",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "docusaurus start",
    "build": "docusaurus build",
    "swizzle": "docusaurus swizzle",
    "write-heading-ids": "docusaurus write-heading-ids",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@docusaurus/core": "catalog:",
    "@docusaurus/preset-classic": "catalog:",
    "@mdx-js/react": "catalog:",
    "clsx": "catalog:",
    "prism-react-renderer": "catalog:",
    "react": "catalog:",
    "react-dom": "catalog:"
  },
  "devDependencies": {
    "@docusaurus/module-type-aliases": "catalog:",
    "@docusaurus/theme-classic": "catalog:",
    "@docusaurus/tsconfig": "catalog:",
    "@docusaurus/types": "catalog:",
    "@nx/devkit": "catalog:",
    "@types/node": "catalog:",
    "markdown-factory": "catalog:",
    "docusaurus-plugin-typedoc": "catalog:",
    "remark-github": "catalog:",
    "typedoc": "catalog:",
    "typedoc-plugin-markdown": "catalog:",
    "typescript": "catalog:"
  },
  "browserslist": {
    "production": [
      ">0.5%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 3 chrome version",
      "last 3 firefox version",
      "last 5 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0"
  },
  "nx": {
    "name": "docs-site",
    "implicitDependencies": [
      "flexi-bench"
    ],
    "targets": {
      "build": {
        "dependsOn": [
          "typecheck",
          "^build"
        ],
        "outputs": [
          "{projectRoot}/.docusaurus",
          "{projectRoot}/build",
          "{projectRoot}/docs/examples",
          "{projectRoot}/docs/api"
        ],
        "inputs": [
          "{projectRoot}/**/*",
          "{projectRoot}/docusaurus.config.js",
          "{workspaceRoot}/packages/flexi-bench/README.md",
          "{workspaceRoot}/packages/flexi-bench/CHANGELOG.md",
          "!{projectRoot}/src/**/*.d.ts",
          "!{projectRoot}/docs/examples",
          "!{projectRoot}/docs/api"
        ]
      },
      "deploy": {
        "dependsOn": [
          "build"
        ],
        "executor": "@nx-dotnet/nx-ghpages:deploy",
        "options": {
          "remote": "https://github.com/agentender/flexi-bench.git",
          "directory": "{projectRoot}/build",
          "baseBranch": "main"
        }
      }
    }
  }
}
